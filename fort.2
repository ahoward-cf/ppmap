#!/bin/bash --login                                                             
#SBATCH --job-name=L1495-01                                
#SBATCH -o L1495-01.log                            
#SBATCH -e L1495-01.err                            
#SBATCH -t 3-00:00                                                              
#SBATCH -p compute                                                              
#SBATCH -n 40                                                                   
#SBATCH --exclusive                                                             
                                                                                
                                                                                
# latest intel compilers, mkl and intel-mpi                                     
                                                                                
module purge                                                                    
module load compiler/intel                                                      
                                                                                
ulimit -s unlimited                                                             
ulimit -c 0                                                                     
                                                                                
code=${HOME}/ppmap/ppmap                                                        
MYPATH=/scratch/c.c1649794                                                      
                                                                                
NCPUS="40"                                                                      
                                                                                
cd ${MYPATH}                                                                    
start="$(date +%s)"                                                             
  for n in $NCPUS; do                                                           
      echo Running PPMAP with OMP_NUM_THREADS=$n                                
      export OMP_NUM_THREADS=$n                                                 
      ${code} L1495                    1   163
      echo PPMAP finished                                                       
  done                                                                          
